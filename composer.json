{
    "name": "faicchia/boring-avatars",
    "description": "Generate boring avatars in PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Pio Faicchia",
            "email": "da.faicchia@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
